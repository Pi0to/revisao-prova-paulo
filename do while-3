#include <stdio.h>

int main() {
    
    int sd, s;
    
    printf("Crie a senha de acesso: ");
    scanf("%d", &s);
    
    
    do {
        printf("Digite a senha novamente para interromper o programa: ");
        scanf("%d", &sd);
        
        if(s == sd) break;
        else printf("Senha incorreta!\n");
    } while(1);
    
    return 0;
}
